datasource db {
  provider = "postgres"
  url      = env("DATABASE_URL")
}

generator client {
  previewFeatures = ["nativeTypes"]
  provider        = "prisma-client-js"
}

// model
model SomeData {
  id       String  @id
  required Boolean
  number   Int     @db.SmallInt
}

model User {
  id     String @id @default(uuid())
  name   String
  mail   String @unique
  status Status @default(ENROLLMENT)
}

enum Status {
  ENROLLMENT
  RECESS
  LEFT
}
